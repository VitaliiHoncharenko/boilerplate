{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "not ie 11",
            "safari >= 10",
            "last 3 Edge versions",
            "not op_mini all",
            "not dead"
          ]
        },
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-external-helpers"
  ]
}
